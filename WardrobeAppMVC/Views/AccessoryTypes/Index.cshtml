@model IEnumerable<WardrobeAppMVC.Models.AccessoryType>

@{
    ViewBag.Title = "Index";
}
<div class="index">
    <div class="container">
        <h2>Index</h2>

        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.AccessoryType1)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.AccessoryType1)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.AccessoryTypeID }) |
                        @Html.ActionLink("Details", "Details", new { id = item.AccessoryTypeID }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.AccessoryTypeID })
                    </td>
                </tr>
            }

        </table>
        <div class="empty-row"></div>
    </div>
</div>